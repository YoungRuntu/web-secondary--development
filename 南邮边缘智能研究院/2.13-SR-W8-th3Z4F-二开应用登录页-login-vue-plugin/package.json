{
  "name": "login-vue-plugin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "plugin": "npm run build && node scripts/genPlugin.js && node scripts/pluginnext",
    "serve": "vue-cli-service serve",
    "sort": "npx sort-package-json",
    "production": "cross-env VUE_APP_NEEDSOURCEMAP=false npm run build && node scripts/genPlugin.js --source=false && node scripts/pluginnext",
    "prebuild": "npm run preserve && node scripts/prebuild",
    "preserve": "node scripts/prestart"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/babel-preset-jsx": "^1.2.4",
    "axios": "^0.23.0",
    "core-js": "^3.6.5",
    "element-ui": "^2.15.6",
    "js-base64": "^3.7.2",
    "js-cookie": "^3.0.1",
    "querystringify": "^2.2.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "adm-zip": "^0.5.9",
    "babel-plugin-component": "^1.1.1",
    "fs-extra": "^10.0.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.6.11",
    "yargs-parser": "^20.2.9",
    "chalk": "4.1.0",
    "cross-env": "^7.0.3"
  }
}